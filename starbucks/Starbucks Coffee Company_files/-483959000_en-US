(function(a){var b="localSiteMsg",c=function(d,e,f){if(_gaq&&_gaq.push){_gaq.push(["_trackEvent","LocationAwareness",d,e,f]);sb.console.log('Tracked location awareness: "'+d+'" with label = "'+e+'" and value = "'+f+'" via Google Analytics')}};if(!a.cookie(b)){a.get("/sysiplocation/localsitemessage",function(f){var i=a("body"),e="local_site_message",d=0,h="DontAskAgain",g;if(f){i.prepend(f);i.addClass(e);g=a("#local_site_message");g.find('input[type="checkbox"]').change(function(){if(a(this).is(":checked")){d=1;a.cookie(b,d,{path:"/",expires:14});c(h,"Clicked",d)}else{d=0;a.cookie(b,null,{path:"/"});c(h,"Clicked",d)}});g.find(".button_cancel button").click(function(j){j.preventDefault();g.remove();i.removeClass(e);c("StayInCurrentSite",h,d)});g.find(".local_site").click(function(j){c("GoToLocalSite",h,d)})}})}}(jQuery));